{
	"$schema": "https://developer.microsoft.com/en-us/json-schemas/office-js/custom-functions.schema.json",
	"functions": [
		{
			"name": "STOCKPRICE",
			"description": "gets stock price",
			"helpUrl": "http://dev.office.com",
			"result": {
				"dimensionality": "scalar",
				"type": "string"
			},
			"parameters": [
				{
					"name": "ticker",
					"description": "stock ticker",
					"type": "string",
					"dimensionality": "scalar"
				}
			],
			"options": {
				"sync": false
			}
		},
		{
			"name": "TRANSLATE",
			"description": "Translates the text to the desired locale",
			"helpUrl": "http://dev.office.com",
			"result": {
				"dimensionality": "scalar",
				"type": "string"
			},
			"parameters": [
				{
					"name": "text",
					"description": "text to localize",
					"type": "string",
					"dimensionality": "matrix"
				},
				{
					"name": "locale",
					"description": "locale code",
					"type": "string",
					"dimensionality": "scalar"
				}
			],
			"options": {
				"sync": false
			}
		},
		{
			"name": "REGEXMATCH",
			"description": "matches regular expression",
			"helpUrl": "http://dev.office.com",
			"result": {
				"dimensionality": "scalar",
				"type": "boolean"
			},
			"parameters": [
				{
					"name": "text",
					"description": "text to localize",
					"type": "string",
					"dimensionality": "scalar"
				},
				{
					"name": "regex",
					"description": "regular expression",
					"type": "string",
					"dimensionality": "scalar"
				}
			],
			"options": {
				"sync": true
			}
		},

		{
			"name": "REGEXREPLACE",
			"description": "replaces regular expression",
			"helpUrl": "http://dev.office.com",
			"result": {
				"dimensionality": "scalar",
				"type": "string"
			},
			"parameters": [
				{
					"name": "text",
					"description": "text to localize",
					"type": "string",
					"dimensionality": "scalar"
				},
				{
					"name": "regex",
					"description": "regular expression",
					"type": "string",
					"dimensionality": "scalar"
				},
				{
					"name": "replace_text",
					"description": "text to replace",
					"type": "string",
					"dimensionality": "scalar"
				}
			],
			"options": {
				"sync": true
			}
		}
	]
}
